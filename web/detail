        function da() {  
            var thisURL = document.URL;
            var  getval =thisURL.split('?')[1];
            var  showval=getval.split("&");
            var x2=showval[1].split("=")[1];
            return x2;
        }
        function na(){
            var thisURL = document.URL;
            var  getval =thisURL.split('?')[1];
            var  showval=getval.split("&");
            var x2=decodeURI(showval[0].split("=")[1]);
            return x2;
        }
        function init(){
            var date=da();
            var name=na();
            var today=getSD();
            var be=getBSD();
            document.getElementById("header").innerText=name;
            document.getElementById("header2").innerText="更新至"+date;
            document.getElementById("inf").innerText=(qz[today]-sw[today]-zy[today]).toString();
            document.getElementById("infect").innerText=qz[today];
            document.getElementById("cure").innerText=zy[today];
            document.getElementById("dead").innerText=sw[today];
            document.getElementById("q").innerText+="+".concat(((qz[today]-sw[today]-zy[today])-(qz[be]-sw[be]-zy[be])).toString());
            document.getElementById("l").innerText+="+"+(qz[today]-qz[be]).toString();
            document.getElementById("c").innerText+="+"+(zy[today]-zy[be]).toString();
            document.getElementById("d").innerText+="+"+(sw[today]-sw[be]).toString();
        }
        function getMonth(day) {
            var x=day.substr(0,day.lastIndexOf('-'));
            x=x.substr(x.indexOf('-')+1,x.length);
            return x;
        }
        function getSD(){
            var x1=da();
            var x2=na();
            var x3=x2.concat(x1);
            return x3;
        }
        function getBSD() {
            var x1=da();
            var x2=x1.substr(x1.length-2,2);
            var x3=x2-1;
            var x4;
            if(x3<10&&x3>=1) {
                x3 = "0".concat(x3.toString());
                x4=x1.substr(0,x1.length-2).concat(x3);
            }
            else if(x3<=0){
                x3=(x3+31)%32;
                var m=getMonth(x1);
                m=m-1;
                m=m.toString();
                m = "0".concat(m.toString());
                x4=x1.substr(0,x1.index('-')+1)+m+"-"+x3;
            }
            else if(x3>=10){
                x3=x3.toString();
                x4=x1.substr(0,x1.length-2).concat(x3);
            }
            x4=na().concat(x4);
            return x4;
        }
        function getFD(day){
            var x1=day;
            var x2=x1.substr(x1.length-2,2);
            var x3=x2-1;
            var x4;
            if(x3<10&&x3>=1) {
                x3 = "0".concat(x3.toString());
                x4=x1.substr(0,x1.length-2).concat(x3);
            }
            else if(x3<=0){
                x3=(x3+31)%32;
                var m=getMonth(x1);
                m=m-1;
                m = "0".concat(m.toString());
                x4=x1.substr(0,4)+"-"+m+"-"+x3;
            }
            else if(x3>10){
                x3=x3.toString();
                x4=x1.substr(0,x1.length-2)+x3;
            }
            return x4;
        }
    </script>
</head>
